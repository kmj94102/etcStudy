<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RentalStatusView">
        <attr name="backgroundRes" format="reference"/>
        <attr name="monthText" format="string"/>
        <attr name="textColorRes" format="reference"/>
        <attr name="dotColorRes" format="reference"/>
    </declare-styleable>
    <declare-styleable name="SelectMonthView">
        <attr name="selectMonthBackgroundRes" format="reference"/>
        <attr name="selectMonthMonthText" format="string"/>
        <attr name="selectMonthTextColorRes" format="reference"/>
        <attr name="selectMonthDotColorRes" format="reference"/>
    </declare-styleable>
    <declare-styleable name="MonthlyCalendarView">
        <attr name="isComplete" format="boolean"/>
        <attr name="isContentsVisible" format="boolean"/>
        <attr name="isEnable" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CustomCalendarItem">
        <attr name="date" format="string"/>
        <attr name="isSelected" format="boolean"/>
    </declare-styleable>
</resources>